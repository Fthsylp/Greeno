@model IEnumerable<Greeno.Models.Domain.PlantContainer>

<div id="plant" class="plants">
    <div class="container">
        <div class="row">
            <div class="col-md-12 ">
                <div class="titlepage">
                    <h2>Our Wonderful plants</h2>
                    <span>looking at its layout. The point of using Lorem Ipsumletters, as opposed to using</span>
                </div>
            </div>
        </div>
    </div>
    <div class="container">
        <div class="row">
            @foreach (var item in Model)
            {
                <div class="col-xl-4 col-lg-4 col-md-6 col-sm-12">
                    <div class="plants-box">
                        <figure><img src="images/plant1.jpg" alt="img" @item.Image /></figure>
                        <h3> @item.Title</h3>
                        <p>@item.Description</p>
                    </div>
                </div>
            }
        </div>
    </div>
</div>
